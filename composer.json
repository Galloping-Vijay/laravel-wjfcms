{
    "name": "galloping-vijay/laravel-wjfcms",
    "type": "project",
    "description": "Laravel-wjfcms is a back-end management system based on laravel5.8 and layui.",
    "keywords": [
        "laravel-wjfcms",
		"cms",
		"php",
        "laravel"
    ],
    "license": "MIT",
	 "authors": [
        {
            "name": "galloping-vijay",
            "email": "1937832819@qq.com",
            "homepage": "https://github.com/Galloping-Vijay/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "^2.17",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^2.37",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
